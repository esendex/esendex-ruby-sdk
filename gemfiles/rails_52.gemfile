source 'http://rubygems.org'

gemspec path: '../'

group :development, :test do
  gem 'activeresource'
  gem 'growl'
  gem 'guard'
  gem 'guard-rspec'
  gem 'listen'
  gem 'rails', '>= 5.2', '< 6.0'
  gem 'rails-observers'
  gem 'rb-fsevent', require: false
  gem 'rb-inotify', require: false
  gem 'rspec'
  gem 'rspec-collection_matchers'
  gem 'rspec-rails'
  gem 'rubocop'
  gem 'wdm', platforms: %i[mswin mingw], require: false
end
